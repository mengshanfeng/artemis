	r2 += i6* (mux1*(u_1[k][j][i-2]-u_1[k][j][i]) + mux2*(u_1[k][j][i-1]-u_1[k][j][i]) + mux3*(u_1[k][j][i+1]-u_1[k][j][i]) + mux4*(u_1[k][j][i+2]-u_1[k][j][i]))*istry;

	cof1=(2*mu[k][j-2][i]+la[k][j-2][i])*met_0[k][j-2][i]*met_0[k][j-2][i]*stry[j-2];
