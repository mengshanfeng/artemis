	cof2 = (2*mu[k-1][j][i]+la[k-1][j][i])*met_2[k-1][j][i]*stry[j]*met_2[k-1][j][i]*stry[j] + mu[k-1][j][i]*(met_1[k-1][j][i]*strx[i]*met_1[k-1][j][i]*strx[i]+ met_3[k-1][j][i]*met_3[k-1][j][i]);
