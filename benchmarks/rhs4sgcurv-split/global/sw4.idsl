	cof1=(mu[k-2][j][i]+la[k-2][j][i])*met_2[k-2][j][i]*met_3[k-2][j][i];
	cof2=(mu[k-1][j][i]+la[k-1][j][i])*met_2[k-1][j][i]*met_3[k-1][j][i];
